{
  "name": "portfolio-server",
  "version": "0.0.0",
  "main": "index.ts",
  "private": true,
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "scripts": {
    "prisma": "npx prisma migrate dev --name init && npx prisma db seed && npx prisma generate",
    "start": "export NODE_ENV=development&  nodemon --verbose --config nodemon.json --exec \"node ./dist/main.js\"",
    "start:prod": "export NODE_ENV=production & node ./dist/main.js",
    "dev": "npm run build:dev && concurrently  \"webpack --config ./webpack/dev.config.js -w\" \"browser-refresh ./dist/main.js\"",
    "build:dev": "webpack --config ./webpack/dev.config.js",
    "build:test": "webpack --config ./webpack/test.config.js -w",
    "build": "webpack --config ./webpack/prod.config.js",
    "compile": "tsc --watch",
    "test": "jest ./dist/tests.test.js -c ./jest.unit.json",
    "test:coverage": "crossenv NODE_ENV=test& jest --coverage -c ./jest.unit.json"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router": "^5.1.18",
    "browser-refresh": "^1.7.3",
    "concurrently": "^7.2.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crossenv": "0.0.2-security",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "http-errors": "^1.6.3",
    "ignore-loader": "^0.1.2",
    "isomorphic": "file:../isomorphic",
    "lasso": "^3.4.0",
    "mongoose": "^5.12.13",
    "morgan": "^1.9.1",
    "net": "^1.0.2",
    "portfinder": "^1.0.28",
    "react": "^17.0.2",
    "react-helmet": "^6.1.0",
    "reflect-metadata": "^0.1.13",
    "stoppable": "^1.1.0",
    "tsyringe": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/react-router-dom": "^5.1.7",
    "@types/stoppable": "^1.1.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "css-loader": "^6.7.1",
    "debug": "^2.6.9",
    "dotenv-webpack": "^7.0.2",
    "jest": "^27.0.0-next.9",
    "mini-css-extract-plugin": "^2.6.0",
    "node-polyfill-webpack-plugin": "^1.1.2",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.7",
    "nodemon-webpack-plugin": "^4.5.2",
    "open": "^8.4.0",
    "prisma": "^3.14.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
